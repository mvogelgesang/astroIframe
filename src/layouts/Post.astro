---
import Layout from "./Layout.astro";
import Post from "../components/Post.astro";
export interface Props {
  content: {
    title: string;
    generator: string;

  }
}

const {content: {title, generator}} = Astro.props;
---

<Layout title={title}>
  <Post title={title} generator={generator}>
    <slot />
  </Post>
</Layout>